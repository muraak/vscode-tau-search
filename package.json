{
    "name": "vscode-tau-search",
    "displayName": "Tau Search",
    "description": "Improve searching on vscode.",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/muraak/vscode-tau-search"
    },
    "icon": "resource/icon.png",
    "publisher": "muraak",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "tau-search-result",
                    "title": "Tau Search Results",
                    "icon": "resource/tau-search-view.svg"
                }
            ]
        },
        "views": {
            "tau-search-result": [
                {
                    "id": "searchResult",
                    "name": "Search Result"
                }
            ]
        },
        "commands": [
            {
                "command": "tau.test",
                "title": "Tau: Test"
            },
            {
                "command": "tau.quickSearch",
                "title": "Tau: Quick Search"
            },
            {
                "command": "tau.detailSearch",
                "title": "Tau: Detail Search"
            },
            {
                "command": "tau.searchResult.deleteNode",
                "title": "Tau: Delete Node(Tree View Mode)"
            },
            {
                "command": "tau.searchResult.renameNode",
                "title": "Tau: Rename Node(Tree View Mode)"
            },
            {
                "command": "tau.moveToNext",
                "title": "Tau: Move to next result(Tree View Mode)"
            },
            {
                "command": "tau.moveToPrevious",
                "title": "Tau: Move to previous result(Tree View Mode)"
            }
        ],
        "menus": {
            "view/item/context": [
                {
                    "command": "tau.searchResult.deleteNode",
                    "when": "view == searchResult"
                },
                {
                    "command": "tau.searchResult.renameNode",
                    "when": "view == searchResult && viewItem != file"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "Tau Search Configurations",
            "properties": {
                "tau.enableFileViewMode": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": true,
                    "description": "Enable to show search result as File View Mode."
                },
                "tau.enableTreeViewMode": {
                    "scope": "resource",
                    "type": "boolean",
                    "default": true,
                    "description": "Enable to show search result as Tree View Mode."
                },
